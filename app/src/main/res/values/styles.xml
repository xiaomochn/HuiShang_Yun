<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <!--
        Base application theme, dependent on API level. This theme is replaced
        by AppBaseTheme from res/values-vXX/styles.xml on newer devices.
    -->
    <style name="AppBaseTheme" parent="android:Theme.Light">
        <!--
            Theme customizations available in newer API levels can go in
            res/values-vXX/styles.xml, while customizations related to
            backward-compatibility can go here.
        -->
    </style>
    
    <!-- 滑动返回主题 -->
    <style name="SwipeBackLayout">
        <item name="edge_size">50dip</item>
        <item name="shadow_left">@drawable/shadow_left</item>
        <item name="shadow_right">@drawable/shadow_right</item>
        <item name="shadow_bottom">@drawable/shadow_bottom</item>
    </style>

    <!-- Application theme. -->
    <style name="AppTheme" parent="AppBaseTheme">
        <item name="android:windowBackground">@color/white</item>
        <!-- All customizations that are NOT specific to a particular API-level can go here. -->
    </style>
    
     <style name="checkbox_style"	parent="@android:style/Widget.CompoundButton.CheckBox">
        <item name="android:button">@drawable/cbox_login</item>
    </style>
    
    <style name="progressdialog" parent="@android:style/Theme.Dialog">
    	<item name="android:windowFrame">@null</item>
    	<item name="android:windowIsFloating">true</item>
    	<item name="android:windowIsTranslucent">false</item>
    	<item name="android:windowNoTitle">true</item>
    	<item name="android:windowBackground">@android:color/transparent</item>
    	<item name="android:backgroundDimEnabled">false</item>
    </style>
    
    <style name="app_pop">
        <item name="android:windowEnterAnimation">@anim/pop_up</item>
        <item name="android:windowExitAnimation">@anim/pop_down</item>
    </style>
    
    
   <style name="popdialog" parent="@android:style/Theme.Dialog">
    	<item name="android:windowFrame">@null</item>
    	<item name="android:windowBackground">@android:color/transparent</item>
    </style>

    <style name="main_tab_bottom">
        <item name="android:textSize">@dimen/text_4</item>
        <item name="android:textColor">#ffffffff</item>
        <item name="android:ellipsize">marquee</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:button">@null</item>
        <item name="android:background">@drawable/rbtn_main</item>
        <item name="android:singleLine">true</item>
        <item name="android:layout_weight">1.0</item>
        <item name="android:drawablePadding">5dp</item>
        <item name="android:padding">@dimen/width_2_80</item>
        <item name="android:minHeight">@dimen/width_5_80</item>
        <item name="android:minWidth">@dimen/width_5_80</item>
    </style>
    
    <style name="app_tab_top">
        <item name="android:textSize">18sp</item>
        <item name="android:textColor">#ffffffff</item>
        <item name="android:ellipsize">marquee</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:button">@null</item>
         <!--<item name="android:background">@drawable/rbtn_main</item>-->
        <item name="android:singleLine">true</item>
        <item name="android:layout_weight">1.0</item>
        <item name="android:drawablePadding">3dp</item>
        <item name="android:paddingTop">20dp</item>
        <item name="android:minHeight">@dimen/width_5_80</item>
        <item name="android:minWidth">@dimen/width_5_80</item>
    </style>
    
    
    <style name="app_regis_top">
        <item name="android:textSize">12sp</item>
        <item name="android:ellipsize">marquee</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:button">@null</item>
        <!-- <item name="android:background">@drawable/rbtn_main</item> -->
        <item name="android:singleLine">true</item>
        <item name="android:layout_weight">1.0</item>
        <item name="android:drawablePadding">2dp</item>
        <item name="android:paddingTop">8dp</item>
        <item name="android:minHeight">@dimen/width_5_80</item>
        <item name="android:minWidth">@dimen/width_5_80</item>
    </style>
    
    <style name="used_image">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:scaleType">fitCenter</item>
    </style>
    
    <style name="used_wrap">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    
    <style name="used_width_match">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    
    <style name="used_height_match">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">match_parent</item>
    </style>
    
    <style name="used_match">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
    </style>
    
   <style name="used_weight_width">
        <item name="android:layout_width">0dip</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    
    <style name="used_weight_height">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">0dip</item>
    </style>
    
    <style name="used_horizontal_line">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">1.0px</item>
    </style>
    
    <style name="used_vertical_line">
        <item name="android:layout_width">1.0px</item>
        <item name="android:layout_height">match_parent</item>
    </style>
    
    <style name="workspace_text">
        <item name="android:textSize">@dimen/text_6</item>
        <item name="android:textColor">@color/m_black</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:singleLine">true</item>
        <item name="android:layout_marginTop">@dimen/width_2_80</item>
    </style>
    
   <style name="workspace_img">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:minHeight">@dimen/width_5_80</item>
        <item name="android:minWidth">@dimen/width_5_80</item>
    </style>
    
    <style name="text_size_7">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">@dimen/text_7</item>
    </style>
    
    <style name="text_size_8">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">@dimen/text_8</item>
    </style>
    
    <style name="text_size_9">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">22sp</item>
    </style>
    
     <style name="text_size_6">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">@dimen/text_6</item>
    </style>
    
  	<style name="used_min_2">
        <item name="android:minHeight">@dimen/width_2_80</item>
        <item name="android:minWidth">@dimen/width_2_80</item>
    </style>
    
   <style name="used_min_3">
        <item name="android:minHeight">@dimen/width_3_80</item>
