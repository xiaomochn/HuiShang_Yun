    <item android:id="@+android:id/background">
        <shape>
            <corners android:radius="10dip" />

            <gradient
                android:angle="270"
                android:centerColor="@color/seektotal"
                android:centerY="0.45"
                android:endColor="@color/seektotal"
                android:startColor="#d4d4d4" />
        </shape>
    </item>
    <item android:id="@+android:id/progress">
        <clip>
            <shape>
                <corners android:radius="10dip" />

                <gradient
                    android:angle="270"
                    android:centerColor="@color/seekbar_bg"
                    android:centerY="0.45"
                    android:endColor="@color/seekbar_bg"
                    android:startColor="@color/seekbar_bg" />
            </shape>
        </clip>
    </item>

</layer-list>