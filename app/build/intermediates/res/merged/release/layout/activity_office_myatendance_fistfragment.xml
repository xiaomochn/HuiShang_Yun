<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#f0f0f0"
    android:orientation="vertical" >

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/height_5_80"
        android:background="#ffffff"
        android:orientation="horizontal" >

        <!-- 一月 -->

        <LinearLayout
            android:layout_width="0.0dp"
            android:layout_height="match_parent"
            android:layout_weight="1.0"
            android:gravity="center" >

            <TextView
                android:id="@+id/Janurary"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="center"
                android:paddingLeft="5dp"
                android:paddingRight="5dp"
                android:text="1月"
                android:textColor="#969696"
                android:textSize="15sp" />
        </LinearLayout>
        <!-- 二月 -->

        <LinearLayout
            android:layout_width="0.0dp"
            android:layout_height="match_parent"
            android:layout_weight="1.0"
            android:gravity="center" >

            <TextView
                android:id="@+id/February"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="center"
                android:paddingLeft="5dp"
                android:paddingRight="5dp"
                android:text="2月"
                android:textColor="#969696"
                android:textSize="15sp" />
        </LinearLayout>
        <!-- 三月 -->

        <LinearLayout
            android:layout_width="0.0dp"
            android:layout_height="match_parent"
            android:layout_weight="1.0"
            android:gravity="center" >

            <TextView
                android:id="@+id/March"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="center"
                android:paddingLeft="5dp"
                android:paddingRight="5dp"
                android:text="3月"
                android:textColor="#969696"
                android:textSize="15sp" />
        </LinearLayout>
        <!-- 四月 -->

        <LinearLayout
            android:layout_width="0.0dp"
            android:layout_height="match_parent"
            android:layout_weight="1.0"
            android:gravity="center" >

            <TextView
                android:id="@+id/April"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="center"
                android:paddingLeft="5dp"
                android:paddingRight="5dp"
                android:text="4月"
                android:textColor="#969696"
                android:textSize="15sp" />
        </LinearLayout>
        <!-- 五月 -->

        <LinearLayout
            android:layout_width="0.0dp"
            android:layout_height="match_parent"
            android:layout_weight="1.0"
            android:gravity="center" >

            <TextView
                android:id="@+id/May"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="center"
                android:paddingLeft="5dp"
                android:paddingRight="5dp"
                android:text="5月"
                android:textColor="#969696"
                android:textSize="15sp" />
        </LinearLayout>
        <!-- 六月 -->

        <LinearLayout
            android:layout_width="0.0dp"
            android:layout_height="match_parent"
            android:layout_weight="1.0"
            android:gravity="center" >

            <TextView
                android:id="@+id/June"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="center"
                android:paddingLeft="5dp"
                android:paddingRight="5dp"
                android:text="6月"
                android:textColor="#969696"
                android:textSize="15sp" />
        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/height_5_80"
        android:background="#ffffff"
        android:orientation="horizontal" >

        <!-- 七月 -->

        <LinearLayout
            android:layout_width="0.0dp"
            android:layout_height="match_parent"
            android:layout_weight="1.0"
            android:gravity="center" >

            <TextView
                android:id="@+id/July"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="center"
                android:paddingLeft="5dp"
                android:paddingRight="5dp"
                android:text="7月"
                android:textColor="#969696"
                android:textSize="15sp" />
        </LinearLayout>
        <!-- 八月 -->

        <LinearLayout
            android:layout_width="0.0dp"
            android:layout_height="match_parent"
            android:layout_weight="1.0"
            android:gravity="center" >

            <TextView
                android:id="@+id/August"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="center"
                android:paddingLeft="5dp"
                android:paddingRight="5dp"
                android:text="8月"
                android:textColor="#969696"
                android:textSize="15sp" />
        </LinearLayout>
        <!-- 九月 -->

        <LinearLayout
            android:layout_width="0.0dp"
            android:layout_height="match_parent"
            android:layout_weight="1.0"
            android:gravity="center" >

            <TextView
                android:id="@+id/September"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="center"
                android:paddingLeft="5dp"
                android:paddingRight="5dp"
                android:text="9月"
                android:textColor="#969696"
                android:textSize="15sp" />
        </LinearLayout>
        <!-- 十月 -->

        <LinearLayout
            android:layout_width="0.0dp"
            android:layout_height="match_parent"
            android:layout_weight="1.0"
            android:gravity="center" >

            <TextView
                android:id="@+id/October"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="center"
                android:paddingLeft="5dp"
                android:paddingRight="5dp"
                android:text="10月"
                android:textColor="#969696"
                android:textSize="15sp" />
        </LinearLayout>
        <!-- 十一月 -->

        <LinearLayout
            android:layout_width="0.0dp"
            android:layout_height="match_parent"
            android:layout_weight="1.0"
            android:gravity="center" >

            <TextView
                android:id="@+id/November"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="center"
                android:paddingLeft="5dp"
                android:paddingRight="5dp"
                android:text="11月"
                android:textColor="#969696"
                android:textSize="15sp" />
        </LinearLayout>
        <!-- 十二月 -->

        <LinearLayout
            android:layout_width="0.0dp"
            android:layout_height="match_parent"
            android:layout_weight="1.0"
            android:gravity="center" >

            <TextView
                android:id="@+id/December"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="center"
                android:paddingLeft="5dp"
                android:paddingRight="5dp"
                android:text="12月"
                android:textColor="#969696"
                android:textSize="15sp" />
        </LinearLayout>
    </LinearLayout>

    <TextView
        android:id="@+id/attendance_name"
        android:layout_width="match_parent"
        android:layout_height="@dimen/height_3_80"
        android:layout_marginTop="@dimen/height_2_80"
        android:gravity="center"
        android:text="胡莉莉"
        android:textColor="#21a5de"
        android:textSize="15sp" />

    <TextView
        android:id="@+id/attendance_month"
        android:layout_width="match_parent"
        android:layout_height="@dimen/height_3_80"
        android:gravity="center"
        android:text="7月份考勤"
        android:textColor="#969696"
        android:textSize="16sp" />

    <LinearLayout
        android:id="@+id/drawlayout"
        android:layout_width="match_parent"
        android:layout_height="@dimen/height_34_80"
        android:layout_gravity="center"
        android:layout_marginTop="@dimen/height_2_80"
        android:background="#f0f0f0"
        android:orientation="vertical">
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/height_3_80"
        android:layout_gravity="center_vertical"
        android:layout_marginLeft="5dp"
        android:layout_marginRight="5dp"
        android:orientation="horizontal" >

        <LinearLayout
            android:layout_width="0.0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1.0"
            android:gravity="center"
            android:orientation="horizontal" >

            <ImageView
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:background="#d44865" />

            <com.huishangyun.Office.Attendance.MarqueeText
                android:id="@+id/askforleave"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="5dp"
                android:ellipsize="end"
                android:text="请假(0.0天)"
                android:textColor="#646464"
                android:textSize="14sp" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="3dp"
            android:gravity="center"
            android:orientation="horizontal" >

            <ImageView
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:background="#b8da8d" />

            <com.huishangyun.Office.Attendance.MarqueeText
                android:id="@+id/later"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="5dp"
                android:ellipsize="end"
                android:text="迟到(0.0天)"
                android:textColor="#646464"
                android:textSize="14sp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="占位"
                android:textColor="#646464"
                android:textSize="14sp"
                android:visibility="invisible" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="0.0dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="3dp"
            android:layout_weight="1.0"
            android:gravity="center"
            android:orientation="horizontal" >

            <ImageView
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:background="#f7a67b" />

            <com.huishangyun.Office.Attendance.MarqueeText
                android:id="@+id/leftearly"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="5dp"
                  android:ellipsize="end"
                android:text="早退(0.0天)"
                android:textColor="#646464"
                android:textSize="14sp" />
        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/height_3_80"
        android:layout_gravity="center_vertical"
        android:layout_marginLeft="5dp"
        android:layout_marginRight="5dp"
        android:layout_marginTop="@dimen/height_1_80"
        android:orientation="horizontal" >

        <LinearLayout
            android:layout_width="0.0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1.0"
            android:gravity="center"
            android:orientation="horizontal" >

            <ImageView
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:background="#4ed5c7" />

            <com.huishangyun.Office.Attendance.MarqueeText
                android:id="@+id/bussinesstrip"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="5dp"
                  android:ellipsize="end"
                android:text="出差(0.0天)"
                android:textColor="#646464"
                android:textSize="14sp" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="3dp"
            android:gravity="center"
            android:orientation="horizontal" >

            <ImageView
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:background="#21a5de" />

            <com.huishangyun.Office.Attendance.MarqueeText
                android:id="@+id/reallyattendance"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="5dp"
                  android:ellipsize="end"
                android:text="实际出勤(0.0天)"
                android:textColor="#646464"
                android:textSize="14sp" />
        </LinearLayout>

        <TextView
            android:layout_width="0.0dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="3dp"
            android:layout_weight="1.0"
            android:textColor="#646464"
            android:textSize="14sp" />
    </LinearLayout>

</LinearLayout>