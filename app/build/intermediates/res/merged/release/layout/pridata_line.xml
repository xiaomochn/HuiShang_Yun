<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    android:gravity="center_vertical" >
    
   <View style="@style/used_horizontal_line"	 />
   
<LinearLayout
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="horizontal"
    android:padding="@dimen/width_2_80"
    android:gravity="center_vertical"
    android:background="@color/h_white" >
    
    <ImageView android:id="@+id/img_pridata_line_icon1"
        android:src="@drawable/icon_profile_tel"	android:scaleType="fitCenter"
        android:layout_width="0.0dip"	android:layout_height="wrap_content"
        style="@style/used_min_4" 	android:contentDescription="@string/hello_world"
        android:layout_weight="0.2" />
    <TextView android:id="@+id/tv_pridata_line_content"
        android:layout_width="0.0dip"	android:layout_height="wrap_content" 
        android:layout_weight="0.6"  android:text="@string/hello_world"
        android:textSize="@dimen/text_4"	android:textColor="@color/theme"/>
    
    <ImageView android:id="@+id/img_pridata_line_icon2"
        android:src="@drawable/icon_totel"	android:scaleType="fitCenter"
        android:layout_width="0.0dip"	android:layout_height="wrap_content"
        style="@style/used_min_4" 	android:contentDescription="@string/hello_world"
        android:layout_weight="0.2" />
   "
</LinearLayout>


</LinearLayout>